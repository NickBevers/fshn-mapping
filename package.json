{
    "name": "fshn-mapping",
    "version": "0.0.1",
    "description": "A library for the mapping of 2D and 3D models on a human body.",
    "main": "/lib/index.js",
    "types": "/lib/index.d.ts",
    "scripts": {
        "build": "tsc",
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest --config jestconfig.json",
        "prepare": "npm run build",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NickBevers/fshn-mapping.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/NickBevers/fshn-mapping/issues"
    },
    "homepage": "https://github.com/NickBevers/fshn-mapping#readme",
    "devDependencies": {
        "prettier": "^2.8.3",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.9.4"
    },
    "files": [
        "lib/**/*"
    ]
}
